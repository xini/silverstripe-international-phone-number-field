{
	"name": "innoweb/silverstripe-international-phone-number-field",
	"description": "Adds a phone filed using Google's libphonenumber.",
	"type": "silverstripe-vendormodule",
	"keywords": ["silverstripe", "form", "database", "field", "phone", "international", "geo-location"],
	"license": "BSD-3-Clause",
	"authors": [{
		"name": "Florian Thoma",
		"homepage": "https://www.innoweb.com.au"
	}],
	"require":
	{
		"silverstripe/framework": "^6",
		"giggsey/libphonenumber-for-php": "^8.13.7"
	},
	"replace": {
		"xini/silverstripe-international-phone-number-field": "self.version"
	},
	"autoload": {
		"psr-4": {
			"Innoweb\\InternationalPhoneNumberField\\": "src/"
		}
    },
    "extra": {
        "expose": [
            "client/dist"
        ]
	}
}
